#!/usr/bin/env node
var app = require('../app');
var db = require('../lib/mysqldb');
var pool = db.getPool()
pool.getConnection(function(err, connection) {
  if(err){
    throw err;
  }
  app.listen(app.config.PORT);
  console.log('Microsservice login listening at http://localhost:%s', app.config.PORT);
});